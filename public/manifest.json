{
    "manifest_version":3,
    "version": "0.0.1",
    "name": "Browse-Smart",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["contentScript.js"]
        }
      ]
}